{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "main",
            "type": "debugpy",
            "request": "launch",
            "env": {
                "PYTHONPYCACHEPREFIX": "${workspaceFolder}/__pycache__",
            },
            "program": "${workspaceFolder}/src/main.py",
            "console": "integratedTerminal"
        },
        {
            "name": "main [PSUTIL_DEBUG]",
            "type": "debugpy",
            "request": "launch",
            "env": {
                "PYTHONPYCACHEPREFIX": "${workspaceFolder}/__pycache__",
                "PSUTIL_DEBUG": "1"
            },
            "program": "${workspaceFolder}/src/main.py",
            "console": "externalTerminal"
        }
    ]
}
